# Supabase Configuration
REACT_APP_SUPABASE_URL=your_supabase_url_here
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# AI-Powered Import Configuration
# Get your free Gemini API key from: https://ai.google.dev/
# Google Gemini offers 1,500 free requests per day
REACT_APP_GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Additional AI providers (for future use)
# REACT_APP_DEEPSEEK_API_KEY=your_deepseek_api_key_here
# REACT_APP_OPENAI_API_KEY=your_openai_api_key_here
# REACT_APP_CLAUDE_API_KEY=your_claude_api_key_here

# AI Configuration Options
REACT_APP_DEFAULT_AI_PROVIDER=gemini
REACT_APP_AI_FALLBACK_ENABLED=true
REACT_APP_AI_MAX_RETRIES=3

# Financial APIs (Optional - free tiers work without these)
# Alpha Vantage - Free: 25 requests/day, Paid: $0.01/request
# Get API key from: https://www.alphavantage.co/support/#api-key
REACT_APP_ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# Real Estate APIs (choose one)
# Attom Data - Free tier: 500 requests/day
# Get API key from: https://developer.attomdata.com/
REACT_APP_ATTOM_API_KEY=your_attom_api_key_here

# Zillow - For real estate property data (paid service)
# Get API key from: https://www.zillow.com/howto/api/iframezillow.htm
REACT_APP_ZILLOW_API_KEY=your_zillow_api_key_here
